{
  "name": "photogabble/laravel-registration-validator",
  "type": "library",
  "description": "Solid credential validation for Laravel.",
  "keywords": [
    "laravel",
    "auth",
    "validation",
    "registration",
    "credentials",
    "username",
    "email"
  ],
  "support": {
    "issues": "https://github.com/photogabble/laravel-registration-validator/issues",
    "source": "https://github.com/photogabble/laravel-registration-validator"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Simon Dann",
      "email": "simon.dann@gmail.com",
      "homepage": "https://www.photogabble.co.uk"
    }
  ],
  "require": {
    "php": ">=7.0.0",
    "photogabble/php-confusable-homoglyphs": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "6.*|7.*",
    "orchestra/testbench": "~3.5"
  },
  "autoload": {
    "psr-4": {
      "Photogabble\\LaravelRegistrationValidator\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Photogabble\\LaravelRegistrationValidator\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Photogabble\\LaravelRegistrationValidator\\ServiceProvider"
      ]
    }
  }
}
